.search-input-group.intk-gallery-search
    search-input(ng-model="searchQuery")
    .search-input-addon
        button(type="button" ng-click="addItem()")
            i.fa.fa-plus

.intk-gallery
    p(class="as-served-set-blank-text" ng-hide="items.length > 0") No items found
    as-served-set(name="item.id"
    description="item.description"
    items="item.images"
    on-removed="onRemoved"
    ng-repeat="item in items track by item.id")

image-select-drawer

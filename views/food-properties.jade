.properties-container#food-properties-container

	.properties-padding
		
		.page-header
			h2 #{ gettext("Food") }

		include forms/food-properties

	.properties-footer
		.input-wrapper
			.btn.success(ng-click='updateFood()')
				span #{ gettext("Accept changes") }
			.btn.warning(ng-click='discardFoodChanges()')
				span #{ gettext("Discard changes") }
			.btn.danger(ng-click='deleteFood()')
				span #{ gettext("Delete") }
			.clearfloat